{
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/",
      "destination": "/templates/webpage.html"
    },
    {
      "source": "/webpage",
      "destination": "/templates/webpage.html"
    },
    {
      "source": "/dashboard",
      "destination": "/templates/unified_dashboard.html"
    },
    {
      "source": "/unified_dashboard",
      "destination": "/templates/unified_dashboard.html"
    },
    {
      "source": "/media",
      "destination": "/templates/media.html"
    },
    {
      "source": "/campaign",
      "destination": "/templates/campaign.html"
    },
    {
      "source": "/forecast",
      "destination": "/templates/forecast.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ]
}

